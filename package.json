{
  "name": "preact-hackathon-starter",
  "version": "0.1.0",
  "description": "Ready-to-go Preact + Redux + firebase hackathon starter project, powered by webpack 2.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --hot --inline --progress",
    "start": "babel-node server",
    "start2": "superstatic build -p 8080 --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "sprestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp build",
    "test": "eslint src tests/**/*.js"
  },
  "keywords": [
    "preact",
    "react",
    "preact-compat",
    "redux",
    "firebase",
    "sass",
    "yarn"
  ],
  "license": "MIT",
  "author": "Ademola Adegbuyi <marhyorh@gmail.com>",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.0.0",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "superstatic": "^4.0.2",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.0",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "firebase": "^3.6.4",
    "material-design-lite": "^1.2.1",
    "preact": "^7.1.0",
    "preact-compat": "^3.9.4",
    "preact-mdl": "^1.3.1",
    "preact-redux": "^1.2.0",
    "preact-render-to-string": "^3.3.0",
    "react-helmet": "^3.2.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0"
  }
}
